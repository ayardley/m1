int main() {
    print("1..3\n");
    
    int i = fact(1);   
    print("ok ");
    print(i);
    i = fact(2); 
    print("\nok ");
    print(i);
    
    i = fact(3);
    i = i / 2;
    print("\nok ");
    print(i);
    print("\n");
}

int fact(int n) {
    if (n == 1)
        return 1;
    else {
        return n * fact(n - 1);
    }
}
