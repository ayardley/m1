
struct integer {
    int val;
}

void main() {
    print("1..2\n");
    integer i = new integer();
    i.val = 42;
    int j = i.val;
    
    if (i.val == 42)
        print("ok 1\n");
    else
        print("not ok 1\n");
        
        
    if (j == i.val && j == 42)
        print("ok 2\n");
    else
        print("not ok 2\n");        
}